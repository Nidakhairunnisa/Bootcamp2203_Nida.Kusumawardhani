
    <h1><%= page %></h1>
    <p>Welcome to my page <%= nama %> </p>
    <h3> Contact List</h3>
    <table class="table">
        <thead class>
		<tr>
			<th scope="col">ID</th>
            <th scope="col">Name</th>
			<th scope="col">Email</th>
		</tr>
		
        <tr>
            <% cont.forEach(function (contact, id)  { id++ %>
            <td><%= id %></td> 
            <td><%= contact.name %></td> 
			<td><%= contact.email %></td> 
            
		</tr>
		    <% }) %>
    </table>

